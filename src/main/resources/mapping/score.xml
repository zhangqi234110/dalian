<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.dalian.mapper.ScoreMapper">
    <select id="findScore" parameterType="string" resultType="com.example.dalian.pojo.Score">

      select * from
      t_score where score_name=#{name}

    </select>



</mapper>